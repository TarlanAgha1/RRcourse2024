# Base path for data
base_path <- "../Data/got/season_"
# Loop through each season to load data and plot
for (i in 1:8) {
# Construct the file path
file_path <- paste0(base_path, i, ".csv")
# Check if the file exists before trying to read
if (file.exists(file_path)) {
# Read the season data
season_data <- read.csv(file_path)
# Create the plot
plot <- ggplot(season_data, aes(x = episode, y = viewers)) +
geom_line(colour = "red", size = 1.5) +
ggtitle(paste("Season", i, "Viewership")) +
xlab("Episode Number") +
ylab("First Day TV Viewers in the US (millions)") +
theme_minimal()
# Print the plot
print(plot)
} else {
print(paste("Data file for Season", i, "not found."))
}
